	publisher = {Association for Computing Machinery},
	author = {Sax, Irmi and Feld, Sebastian and Zielinski, Sebastian and Gabor, Thomas and Linnhoff-Popien, Claudia and Mauerer, Wolfgang},
	month = may,
	year = {2020},
	keywords = {quantum annealing, approximation, NP-complete problems, simplifying QUBOS},
	pages = {108--117},
	file = {Sax et al. - 2020 - Approximate approximation on a quantum annealer.pdf:F\:\\App\\Sync\\Sync\\Studia\\ICM\\Quantum\\Zotero\\storage\\Z7VZGLL8\\Sax et al. - 2020 - Approximate approximation on a quantum annealer.pdf:application/pdf},
